{
  "name": "daq-test-backend",
  "version": "0.0.1",
  "engines": {
    "node": ">=20"
  },
  "description": "DAQ test backend",
  "author": "Dylan",
  "private": true,
  "license": "LICENSED",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings=0",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "processors:dev:suggestion": "ts-node -r tsconfig-paths/register ./src/processors/suggestion.processor.ts",
    "processors:dev:init-admin": "ts-node -r tsconfig-paths/register ./src/processors/init-admin.processor.ts",
    "processors:prod:suggestion": "node dist/processors/suggestion.processor.js",
    "typeorm:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/configs/database.config.ts",
    "typeorm:migrate:prod": "node node_modules/typeorm/cli.js migration:run -d ./dist/configs/database.config.js",
    "typeorm:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/configs/database.config.ts",
    "typeorm:create": "cd src/database/migrations && ts-node -r tsconfig-paths/register ./../../../node_modules/typeorm/cli.js migration:create",
    "test": "jest --silent=false",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.777.0",
    "@aws-sdk/s3-request-presigner": "^3.777.0",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/serve-static": "^5.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/multer": "^1.4.12",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "bech32": "2.0.0",
    "bignumber.js": "^9.1.2",
    "bullmq": "^5.18.0",
    "cache-manager": "^5.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "google-auth-library": "^9.15.1",
    "helmet": "^6.1.5",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^4.1.0",
    "openai": "^5.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "4.1.3",
    "typeorm": "^0.3.15",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@moneteam/eslint-plugin-nestjs": "^1.6.0",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@trivago/prettier-plugin-sort-imports": "4.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^16.1.2",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.5"
  }
}
